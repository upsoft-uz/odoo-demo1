
<odoo>
  <data noupdate="1">
    <record id="ir_cron_edu_penalty" model="ir.cron">
      <field name="name">Edu: Recompute Penalties</field>
      <field name="model_id" ref="model_edu_contract"/>
      <field name="state">code</field>
      <field name="code">model.cron_recompute_penalties()</field>
      <field name="interval_number">1</field>
      <field name="interval_type">days</field>
      <field name="numbercall">-1</field>
      <field name="active">True</field>
    </record>
    <record id="seq_edu_contract" model="ir.sequence">
      <field name="name">Edu Contract</field>
      <field name="code">edu.contract</field>
      <field name="prefix">CONTR-%(y)s-</field>
      <field name="padding">5</field>
    </record>
  </data>
</odoo>
